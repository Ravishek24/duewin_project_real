# Database Migration Configuration
# Copy this to .env and fill in your actual values

# Source Database (Current Database)
SOURCE_DB_HOST=your-current-db-host.com
SOURCE_DB_PORT=3306
SOURCE_DB_NAME=duewin
SOURCE_DB_USER=migration_user
SOURCE_DB_PASSWORD=your_migration_password

# Target Database (New Aurora Cluster)
TARGET_DB_HOST=your-aurora-cluster.cluster-xyz.region.rds.amazonaws.com
TARGET_DB_PORT=3306
TARGET_DB_NAME=duewin
TARGET_DB_USER=admin
TARGET_DB_PASSWORD=your_aurora_password

# Migration Settings
MIGRATION_BATCH_SIZE=1000
MIGRATION_TIMEOUT=300000
MIGRATION_RETRY_ATTEMPTS=3 